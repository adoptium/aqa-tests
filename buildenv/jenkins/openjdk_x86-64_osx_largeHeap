#!groovy
LABEL='sw.os.osx&&hw.arch.x86'

node ("master") {
	checkout scm
	def jenkinsfile = load "${WORKSPACE}/openjdk-tests/buildenv/jenkins/JenkinsfileBase"
	jenkinsfile.setLabelParam()
	cleanWs()
}

node("$LABEL") {
    PLATFORM = 'x64_mac'
    SDK_RESOURCE = 'upstream'
    SPEC='osx_x86-64'
    checkout scm
    def jenkinsfile = load "${WORKSPACE}/openjdk-tests/buildenv/jenkins/JenkinsfileBase"
    jenkinsfile.testBuild()
}