<playlist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../TKG/playlist.xsd">
	<test>
		<testCaseName>jenkins_test</testCaseName>
		<command> $(TEST_ROOT)$(D)external$(D)external.sh --run --tag nightly --version ${JDK_VERSION} --impl ${JDK_IMPL} --dir jenkins --report $(REPORTDIR); \
		$(TEST_ROOT)$(D)external$(D)external.sh --clean --tag nightly --version ${JDK_VERSION} --impl ${JDK_IMPL} --dir jenkins; \
		$(TEST_STATUS)</command>
		<levels>
			<level>extended</level>
		</levels>
		<groups>
			<group>external</group>
		</groups>
	</test>
	<test>
		<testCaseName>jenkins_test_with_tag</testCaseName>
		<command> $(TEST_ROOT)$(D)external$(D)external.sh --run --tag ${DOCKERIMAGE_TAG} --version ${JDK_VERSION} --impl ${JDK_IMPL} --dir jenkins --report $(REPORTDIR); \
		$(TEST_ROOT)$(D)external$(D)external.sh --clean --tag ${DOCKERIMAGE_TAG} --version ${JDK_VERSION} --impl ${JDK_IMPL} --dir jenkins; \
		$(TEST_STATUS)</command>
		<levels>
			<level>extended</level>
		</levels>
		<groups>
			<group>external</group>
		</groups>
	</test>
</playlist>
