<playlist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../TKG/playlist.xsd">
	 <test>
                <testCaseName>sanity_system</testCaseName>
                <command>docker run --name system-test adoptopenjdk-system-test:latest sanity.system; \
                 docker rm system-test; \
                $(TEST_STATUS)</command>
                <subsets>
                        <subset>8</subset>
                        <subset>9</subset>
                </subsets>
                <levels>
                        <level>special</level>
                </levels>
                <groups>
                        <group>system</group>
                </groups>
        </test>

	 <test>
                <testCaseName>extended_system</testCaseName>
                <command>docker run --name system-test adoptopenjdk-system-test:latest extended.system; \
                 docker rm system-test; \
                $(TEST_STATUS)</command>
                <subsets>
                        <subset>8</subset>
                        <subset>9</subset>
                </subsets>
                <levels>
                        <level>special</level>
                </levels>
                <groups>
                        <group>system</group>
                </groups>
        </test>
</playlist>
