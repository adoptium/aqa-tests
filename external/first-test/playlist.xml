<playlist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../TKG/playlist.xsd">

	<!--Add entries corresponding to your test similar to the example below so that
	    TestKitGen can produce the equivalent make command lines to be executed in the build 
	-->
	<test>
		<testCaseName>first-test</testCaseName>
		<command>docker run --rm $(EXTRA_DOCKER_ARGS) adoptopenjdk-first-test:latest ; \
			$(TEST_STATUS); \
			docker rmi -f adoptopenjdk-first-test
		</command>
		<levels>
			<level>sanity</level>
			<level>extended</level>
			<level>special</level>
		</levels>
		<groups>
			<group>external</group>
		</groups>
	</test>
	
	
</playlist>
