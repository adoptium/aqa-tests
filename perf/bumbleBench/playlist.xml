	<!-- GPU Benchmarks -->
	<test>
		<testCaseName>bumbleBench-BitonicBench-CPU</testCaseName>
		<command>$(JAVA_COMMAND) -jar $(TEST_RESROOT)/bumblebench/BumbleBench.jar BitonicBench.CPU; \
		$(TEST_STATUS)</command>
		<platformRequirements>os.linux,arch.x86,bits.64</platformRequirements>
		<subsets>
			<subset>8</subset>
			<subset>11</subset>
		</subsets>		
		<levels>
			<level>special</level>
		</levels>
		<groups>
