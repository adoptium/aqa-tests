<playlist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../TKG/resources/playlist.xsd">
	<test>
		<testCaseName>ssl-tests</testCaseName>
		<disables>
			<disable>
				<comment>https://github.com/eclipse-openj9/openj9/issues/19164</comment>
				<platform>.*mac.*</platform>
				<impl>openj9</impl>
			</disable>
		</disables>
		<command>
			JTREG_HOME=$(Q)$(JTREG_HOME)$(Q) \
			$(TEST_ROOT)$(D)functional$(D)security$(D)ssl-tests$(D)ssl-tests$(D)run.sh $(Q)$(TEST_JDK_HOME)$(Q) ; $(TEST_STATUS)
		</command>
		<levels>
			<level>dev</level>
		</levels>
		<platformRequirementsList>
			<platformRequirements>os.linux</platformRequirements>
			<platformRequirements>os.win</platformRequirements>
			<platformRequirements>os.osx</platformRequirements>
		</platformRequirementsList>
		<groups>
			<group>functional</group>
		</groups>
	</test>
</playlist>
